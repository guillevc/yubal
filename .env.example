# Server
YUBAL_HOST=0.0.0.0
YUBAL_PORT=8000
YUBAL_RELOAD=false
YUBAL_DEBUG=false

# Paths
YUBAL_DATA_DIR=./data
YUBAL_BEETS_DIR=./beets
YUBAL_YTDLP_DIR=./ytdlp

# Audio (opus = best quality/size, no transcoding; options: opus, m4a, mp3, flac, best)
YUBAL_AUDIO_FORMAT=opus
YUBAL_AUDIO_QUALITY=0

# Timezone (IANA format, e.g., "UTC", "America/New_York", "Europe/London")
YUBAL_TZ=UTC
